{
    "name": "Smart Factory (High Precision)",
    "description": "High-frequency industrial monitoring for predictive maintenance",
    "protocol": "YOUR_PROTOCOL",
    "mode": "active",
    "server_ip": "127.0.0.1",
    "server_port": 5000,
    "num_clients": 10,
    "duration": 60,
    "packets_per_client": 60,
    "sensors": ["vibration", "temperature", "pressure", "rpm"],
    "traffic_pattern": {
        "vibration": {"rate_hz": 10.0, "burst": true},     
        "temperature": {"rate_hz": 1.0, "burst": false},
        "pressure": {"rate_hz": 5.0, "burst": false},
        "rpm": {"rate_hz": 2.0, "burst": false}     
    },
    "network_conditions": {
        "latency_ms": 5,
        "jitter_ms": 2,
        "bandwidth_kbps": 10000,
        "loss_percent": 0.1
    },
    "failure_injection": {
        "packet_loss": 0.001,  
        "client_crashes": []
    },
    "network_profile": "perfect"
}
