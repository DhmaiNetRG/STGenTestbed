{
    "name": "Healthcare Wearables",
    "description": "Patient monitoring with critical health data streams",
    "protocol": "YOUR_PROTOCOL",
    "mode": "active",
    "server_ip": "127.0.0.1",
    "server_port": 5000,
    "num_clients": 4,
    "duration": 60,
    "packets_per_client": 60,
    "sensors": ["heart_rate", "spo2", "temperature", "accelerometer"],
    "traffic_pattern": {
        "heart_rate": {"rate_hz": 1.0, "burst": false},     
        "spo2": {"rate_hz": 0.5, "burst": false},
        "temperature": {"rate_hz": 0.1, "burst": false},
        "accelerometer": {"rate_hz": 5.0, "burst": true}     
    },
    "network_conditions": {
        "latency_ms": 15,
        "jitter_ms": 3,
        "bandwidth_kbps": 2000,
        "loss_percent": 0.5
    },
    "failure_injection": {
        "packet_loss": 0.01,  
        "client_crashes": []
    },
    "network_profile": "wifi"
}
